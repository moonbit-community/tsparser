///|
pub enum Keyword {
  Let
  Const
  Var
  Function
  Return
  If
  Else
  While
  For
  Do
  Switch
  Case
  Type
  Typeof
  Interface
  Enum
  True
  False
  Null
  Undefined
  This
  Delete
  Void
  Await
  Yield
  New
